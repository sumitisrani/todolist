<%= render "home/home" %>
<legend>Project list</legend>
 
<table class="table table-striped">
  <tr>
    <th>Title</th>
    <th>Id</th>
    <th>Project</th>
    <th>Task</th>  
  </tr>
 
<% @projects.each do |project| %>
  <tr>
    <td><%= project.title %></td>
    <td><%= project.id %></td>
    <td><%= link_to 'Go To Project', project_path(project.id) ,:class =>"btn btn-primary"%></td>
    <td><%= link_to 'show tasks',project_tasks_path(project.id),:class =>"btn btn-primary"%></td>
  </tr>
<% end %>
</table>
