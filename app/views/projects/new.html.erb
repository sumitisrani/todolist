<%= render "home/home" %>
<legend>new Project</legend>
<% @project.errors.full_messages.each do |msg| %>
       <li><%= msg %></li>
 <% end %>

<%= form_for @project,url:{action:"create"}  do |f| %>
  <div><%= f.label :title %>
  <%= f.text_field :title %></div>
  
  <div><%= f.label :startdate %>
  <%=date_select("project", "start_date", :order => [:day, :month, :year])%></div>
  
  <div><%= f.label :enddate %>
  <%=date_select("project", "end_date", :order => [:day, :month, :year])%></div>

  <div><%= f.label :status %>
  <%= f.text_field :status %></div>

  <div><%= f.label :description %>
  <%= f.text_area :description, :cols => "40", :rows => "10"  %></div>

  <div><%= f.submit "create project" %></div>
 
<% end %>

